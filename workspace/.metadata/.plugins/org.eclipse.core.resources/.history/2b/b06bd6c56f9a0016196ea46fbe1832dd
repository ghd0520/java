package kr.hs.dgsw.ch13;

public class TwoDimArray {

	public static void main(String[] args) {
		
		int [][] arr = new int[3][];//선언과 생성
		//초기화
		arr[0] = new int[5];
		arr[1] = new int[5];
		arr[2] = new int[5];
		//배열의 사용
		for(int[] row : arr){
			for(int i : row){
				System.out.print(i + " ");
			}
			System.out.println();
		}
	}

}
